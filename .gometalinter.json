{
  "Enable": [
    "deadcode",
    "maligned",
    "errcheck",
    "gosec",
    "goconst",
    "gocyclo",
    "golint",
    "lll",
    "megacheck",
    "ineffassign",
    "interfacer",
    "misspell",
    "structcheck",
    "unconvert",
    "varcheck",
    "vet",
    "vetshadow"
  ],
  "VendoredLinters": true,
  "Vendor": true,
  "Concurrency": 4,
  "LineLength": 100,
  "Format": "({{.Linter}}): {{.Severity}}: {{.Path}}:{{.Line}}:{{if .Col}}{{.Col}}{{end}}: {{.Message}}",
  "Exclude": [
    ".+vendor/.+",
    ".+pb\\.go"
  ]
}
