{
  "Enable": [
    "dupl",
  ],
  "VendoredLinters": true,
  "Vendor": true,
  "Concurrency": 2,
  "Format": "({{.Linter}}): {{.Severity}}: {{.Path}}:{{.Line}}:{{if .Col}}{{.Col}}{{end}}: {{.Message}}",
  "Linters": {
    "dupl": "dupl -plumbing -threshold {duplthreshold} {path}:^(?P<path>.*?[^pb\\.]{3}\\.go):(?P<line>\\d+)-\\d+:\\s*(?P<message>.*)$",
  }
}