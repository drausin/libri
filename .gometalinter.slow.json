{
  "Enable": [
    "deadcode",
    "gas",
    "goconst",
    "gosimple",
    "misspell",
    "staticcheck",
    "structcheck",
    "unused"
  ],
  "VendoredLinters": true,
  "Vendor": true,
  "Concurrency": 2,
  "Format": "({{.Linter}}): {{.Severity}}: {{.Path}}:{{.Line}}:{{if .Col}}{{.Col}}{{end}}: {{.Message}}",
  "Linters": {
    "unused": "unused {path}:^(?P<path>.*?[^filtered_logger]\\.go):(?P<line>\\d+):(?P<col>\\d+):\\s*(?P<message>.*)$"
  }
}
